{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "jsx": "react-jsx",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./dist",
    "sourceMap": true
  },
  "include": ["views/**/*"],
  "exclude": [
    "node_modules"
  ]
}
